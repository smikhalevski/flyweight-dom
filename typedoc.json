{
  "name": "Flyweight DOM",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeExternals": true,
  "disableSources": true,
  "hideGenerator": true,
  "readme": "none",
  "tsconfig": "./tsconfig.json",
  "customCss": "./node_modules/typedoc-custom-css/custom.css",
  "navigation": {
    "includeFolders": false
  },
  "entryPoints": ["./src/main/index.ts", "./src/main/dsl.ts"],
  "compilerOptions": {
    "skipLibCheck": true
  },
  "plugin": ["typedoc-plugin-mdn-links"]
}
